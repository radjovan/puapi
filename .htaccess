<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /programirano_ucenje
  
  # Ako tra≈æeni fajl ili direktorijum postoji, prosledi ga
  RewriteCond %{REQUEST_FILENAME} -f [OR]
  RewriteCond %{REQUEST_FILENAME} -d
  RewriteRule ^ - [L]

  # Preusmeri sve ostale zahteve na index.html
  RewriteRule ^ index.html [L]
</IfModule>
